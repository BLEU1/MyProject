#include <stdio.h>
#include <stdlib.h>
int n1,n2;
struct course *head1;
struct student *head2;

//课程信息结构体
struct course
{
	int num1; //课程编号
	char name1[50]; //课程名称
	int type; //课程性质
	int hour; //授课学时
	int score; //学分
	int selectpeo; //课程已选人数
	int toplimit; //课程人数上限
	struct course *next;
};

//学生信息结构体
struct student
{
	int num2; //学号
	char name2[50]; //姓名
	char selectnum[50]; //已选课程列表
	int selectcount; //已选课程数量
	struct student *next;
};
